{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": "off",
    //"object-curly-spacing": "off",
    "max-len": "off",
    "semi": "off",
    "require-jsdoc": "off",
    "no-trailing-spaces": "off",
    "quotes": "off",
    "space-before-blocks": "off",
    "eol-last": "off",
    "spaced-comment": "off",
    "operator-linebreak": "off",
    "indent": "off",
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}