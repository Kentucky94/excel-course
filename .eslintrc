{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "arrow-parens": "off",
    //"quotes": "off",
    "comma-dangle": "off",
    //"object-curly-spacing": "off",
    "max-len": "off",
    "semi": "off",
    //"eol-last": "off",
    "require-jsdoc": "off",
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}